<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.1" />
	</ItemGroup>

	<PropertyGroup>
		<PackageId>AutomaticDependencyInjectionRegistration</PackageId>
		<Version>1.0.0</Version>
		<Authors>VictorGrego</Authors>
		<Description>Extensões para registro automático de serviços usando atributos personalizados.</Description>
		<PackageTags>dependency-injection;di;automatic-registration</PackageTags>
		<RepositoryUrl>https://github.com/victorgregono/AutomaticDependencyInjectionRegistration.git</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

</Project>
